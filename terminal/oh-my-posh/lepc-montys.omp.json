{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#3b3b3b",
          "foreground": "#b1b1b1",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": "<#ffffff>\uf120</> {{ .Name }} ",
          "type": "shell"
        },
        {
          "background": "#626262",
          "foreground": "#b1b1b1",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " <#ffffff>{{ .Icon }}</> {{ .HostName }} ",
          "type": "os"
        },
        {
          "background": "#3b3b3b",
          "foreground": "#b1b1b1",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " <#ffffff>\ufa1a\u2800</>{{ .CurrentDate | date .Format }} ",
          "type": "time"
        },
        {
          "background": "#626262",
          "foreground": "#b1b1b1",
          "properties": {
            "always_enabled": true
          },
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " <#ffffff>\uf251</> {{ .FormattedMs }} ",
          "type": "executiontime"
        },
        {
          "background": "#3b3b3b",
          "foreground": "#b1b1b1",
          "properties": {
            "folder_icon": "\uf6d7",
            "folder_separator_icon": "<#000>\uf054</>",
            "home_icon": "\uf7db",
            "style": "agnoster_short"
          },
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " <#ffffff>\uf755</> {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "#fffb38",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FF9248{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#ff4500{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B388FF{{ end }}"
          ],
          "foreground": "#193549",
          "leading_diamond": "\ue0b6",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "branch_max_length": 25,
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }} ",
          "trailing_diamond": "\ue0b4",
          "type": "git"
        },
        {
          "background": "#4b6043",
          "foreground": "#b1b1b1",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " <#ffffff>\ue718</> {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "type": "node"
        },
	{
          "background": "#626262",
          "foreground": "#b1b1b1",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " <#ffffff>\ufd31</> {{if .Namespace}}{{.Namespace}}{{else}}default{{end}} ",
          "type": "kubectl"
        },
	{
          "background": "#3b3b3b",
          "foreground": "#b1b1b1",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " <#ffffff>\ue7ad</> {{.Profile}}{{if .Region}}<#000>@</>{{.Region}}{{end}} ",
          "type": "aws"
        },
        {
          "background": "#0b6a3c",
          "background_templates": [
            "{{ if gt .Code 0 }}red{{ end }}"
          ],
          "foreground": "#ffffff",
          "properties": {
            "always_enabled": true
          },
          "powerline_symbol": "\ue0b0",
          "style": "diamond",
          "template": " {{ if gt .Code 0 }}\uf467{{ else }}\uf42e{{ end }}",		  
          "trailing_diamond": "\ue0b4",
          "type": "exit"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#cd5e42",
          "style": "plain",
          "template": "\ue3bf\u2800",
          "type": "root"
        },
        {
          "foreground": "#239b5d",
          "style": "plain",
          "template": "<#0b6a3c>\u26a1</>{{if .Root}}<#cd5e42>{{ .UserName }}</>{{else}}{{ .UserName }}{{end}} <#0b6a3c>\u276f</>",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
