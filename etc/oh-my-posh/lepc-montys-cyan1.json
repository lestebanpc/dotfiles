{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "shell",
          "style": "diamond",
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "template": "\uf120 {{ .Name }} ",
          "foreground": "#b1b1b1",
          "background": "#3b3b3b"
        },
        {
          "type": "os",
          "style": "powerline",
          "properties": {
            "cache_duration": "none"
          },
          "template": " {{ .Icon }} {{ .HostName }} ",
          "foreground": "#b1b1b1",
          "powerline_symbol": "\ue0b0",
          "background": "#626262"
        },
        {
          "type": "time",
          "style": "powerline",
          "properties": {
            "cache_duration": "none"
          },
          "template": " \udb81\udd1b {{ .CurrentDate | date .Format }} ",
          "foreground": "#b1b1b1",
          "powerline_symbol": "\ue0b0",
          "background": "#3b3b3b"
        },
        {
          "type": "executiontime",
          "style": "powerline",
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": " \uf251 {{ .FormattedMs }} ",
          "foreground": "#b1b1b1",
          "powerline_symbol": "\ue0b0",
          "background": "#626262"
        },
        {
          "type": "path",
          "style": "powerline",
          "properties": {
            "cache_duration": "none",
            "folder_icon": "\udb80\uddd8",
            "folder_separator_icon": "<#000>\uf054</>",
            "home_icon": "\uf4e2",
            "style": "agnoster_short"
          },
          "template": " \uf114 {{ .Path }} ",
          "foreground": "#b1b1b1",
          "powerline_symbol": "\ue0b0",
          "background": "#3b3b3b"
        },
        {
          "type": "git",
          "style": "powerline",
          "properties": {
            "branch_max_length": 25,
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "native_fallback": true
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }} ",
          "foreground": "#b1b1b1",
          "powerline_symbol": "\ue0b0",
          "background": "#002800",
          "background_templates": [
            "{{ if .Working.Changed }}#973d46{{ end }}",
            "{{ if .Staging.Changed }}#3f0001{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#a35a00{{ end }}",
            "{{ if gt .Ahead 0 }}#235284{{ end }}",
            "{{ if gt .Behind 0 }}#84791b{{ end }}"
          ]
        },
        {
          "type": "go",
          "style": "powerline",
          "properties": {
            "cache_duration": "none"
          },
          "template": " \ue626 {{ .Full }}",
          "foreground": "#b1b1b1",
          "powerline_symbol": "\ue0b0",
          "background": "#18848e"
        },
        {
          "type": "dotnet",
          "style": "powerline",
          "properties": {
            "cache_duration": "none"
          },
          "template": " \ue77f {{ .Full }}",
          "foreground": "#b1b1b1",
          "powerline_symbol": "\ue0b0",
          "background": "#406182"
        },
        {
          "type": "node",
          "style": "powerline",
          "properties": {
            "cache_duration": "none"
          },
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "foreground": "#b1b1b1",
          "powerline_symbol": "\ue0b0",
          "background": "#4d807f"
        },
        {
          "type": "python",
          "style": "powerline",
          "properties": {
            "cache_duration": "none"
          },
          "template": " \ue235 {{ if .Venv }}{{ .Venv }}<#000000>\uf054</>{{ end }}{{ .Full }} ",
          "foreground": "#b1b1b1",
          "powerline_symbol": "\ue0b0",
          "background": "#4e2c5d"
        },
        {
          "type": "kubectl",
          "style": "powerline",
          "properties": {
            "cache_duration": "none"
          },
          "template": " \udb84\udcfe {{if .Namespace}}{{.Namespace}}{{else}}default{{end}} ",
          "foreground": "#b1b1b1",
          "powerline_symbol": "\ue0b0",
          "background": "#626262"
        },
        {
          "type": "aws",
          "style": "powerline",
          "properties": {
            "cache_duration": "none"
          },
          "template": " \ue7ad {{.Profile}}{{if .Region}}<#000>@</>{{.Region}}{{end}} ",
          "foreground": "#b1b1b1",
          "powerline_symbol": "\ue0b0",
          "background": "#3b3b3b"
        },
        {
          "type": "status",
          "style": "diamond",
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "trailing_diamond": "\ue0b4",
          "template": " {{ if gt .Code 0 }}\uf467{{ else }}\uf42e{{ end }}",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#015e6d",
          "background_templates": ["{{ if gt .Code 0 }}red{{ end }}"]
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "root",
          "style": "plain",
          "properties": {
            "cache_duration": "none"
          },
          "template": "\ue3bf ",
          "foreground": "#259797"
        },
        {
          "type": "text",
          "style": "plain",
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#015e6d>\udb81\uded5</> {{.UserName}} <#015e6d>‚ùØ</>",
          "foreground": "#259797"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "shell_integration": true,
  "final_space": true
}
